#!/bin/bash
#SBATCH --partition=debug
#SBATCH --job-name="P3RR"
#SBATCH --output=P3RR-%j.out
#SBATCH --error=P3RR-%j.err
#SBATCH --nodes=2
#SBATCH --ntasks-per-node=12
##SBATCH --mem=4800
#SBATCH --time=00:30:00
#SBATCH --constraint=IB&CPU-E5645
#SBATCH --mail-user=ninastei@buffalo.edu
#SBATCH --mail-type=ALL
#SBATCH --requeue

module load intel/17.0
module load intel-mpi/2017.0.1
module list

export I_MPI_PMI_LIBRARY=/usr/lib64/libpmi.so

#srun ../execs/P3P3a.mpi>P3P3a.txt
srun ../execs/P3RRb.mpi<../execs/coarsein.txt>P3RRbcoarse.txt
srun ../execs/P3RRb.mpi<../execs/medin.txt>P3RRbmed.txt
srun ../execs/P3RRb.mpi<../execs/finein.txt>P3RRbfine.txt
#srun ../execs/P3RRc.mpi>P3RRc.txt
echo "AND LO!"
